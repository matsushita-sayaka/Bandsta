<h1>You wan to replace this file?</h1>
<br><br>

  <%= form_for @band_detail, :html => { multipart: true } do |f| %>
  
  
    <div class="field">
      <%= f.label :file_name %><br />
      <ul class="file_post">
        <li class="file_post">
          <%= f.file_field :file_name, {:class => "form-control my-form"} %>
        </li>
      </ul>
    </div>
  
  <br>
  
    <div class="field">
      <%= f.label :Comment %><br />
      <%= f.text_area :file_exp, :class => "form-control my-form" %>
    </div>  
  
  <br>
    <div class="actions">
      <%= f.submit "Post!", :class => "btn btn-default" %>
    </div>
  <% end%>
